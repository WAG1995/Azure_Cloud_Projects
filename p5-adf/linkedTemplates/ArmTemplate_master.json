{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "p5-adf"
		},
		"AzureDataLakeStorage1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage1'"
		},
		"p5_coc_databrick_ls_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'p5_coc_databrick_ls'"
		},
		"AzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://p5datalake.dfs.core.windows.net/"
		},
		"coc_api_ls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://api.clashofclans.com/v1/clans/%232LRYP09Q/capitalraidseasons"
		},
		"p5_coc_databrick_ls_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "0404-102052-2q1qt7jj"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "p5-adf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorage1_accountKey": {
						"value": "[parameters('AzureDataLakeStorage1_accountKey')]"
					},
					"p5_coc_databrick_ls_accessToken": {
						"value": "[parameters('p5_coc_databrick_ls_accessToken')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_url')]"
					},
					"p5_coc_databrick_ls_properties_typeProperties_existingClusterId": {
						"value": "[parameters('p5_coc_databrick_ls_properties_typeProperties_existingClusterId')]"
					},
					"coc_api_ls_properties_typeProperties_url": {
						"value": "[parameters('coc_api_ls_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}