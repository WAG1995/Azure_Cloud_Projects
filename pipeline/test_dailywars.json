{
	"name": "test_dailywars",
	"properties": {
		"activities": [
			{
				"name": "test daily wars",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [
					{
						"name": "Source",
						"value": "bronze/dailyWar_raw/dailyWar_raw.json"
					},
					{
						"name": "Destination",
						"value": "dailywar-silver/warNotEnded/@{concat(formatDateTime(utcNow(), 'dd-MM-yyyy'),'.parquet')}"
					}
				],
				"typeProperties": {
					"source": {
						"type": "JsonSource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "JsonReadSettings"
						}
					},
					"sink": {
						"type": "ParquetSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						},
						"formatSettings": {
							"type": "ParquetWriteSettings"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "$['state']"
								},
								"sink": {
									"name": "state"
								}
							},
							{
								"source": {
									"path": "$['teamSize']"
								},
								"sink": {
									"name": "teamSize"
								}
							},
							{
								"source": {
									"path": "$['attacksPerMember']"
								},
								"sink": {
									"name": "attacksPerMember"
								}
							},
							{
								"source": {
									"path": "$['preparationStartTime']"
								},
								"sink": {
									"name": "preparationStartTime"
								}
							},
							{
								"source": {
									"path": "$['startTime']"
								},
								"sink": {
									"name": "startTime"
								}
							},
							{
								"source": {
									"path": "$['endTime']"
								},
								"sink": {
									"name": "endTime"
								}
							}
						],
						"collectionReference": "",
						"mapComplexValuesToString": true
					}
				},
				"inputs": [
					{
						"referenceName": "dailyWars_dataSink",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "war_not_ended_data",
						"type": "DatasetReference"
					}
				]
			},
			{
				"name": "If Condition1",
				"type": "IfCondition",
				"dependsOn": [
					{
						"activity": "daily war sink lookup",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"userProperties": [],
				"typeProperties": {
					"expression": {
						"value": "@equals(activity('daily war sink lookup').output.firstRow.state, 'inWar')",
						"type": "Expression"
					},
					"ifTrueActivities": [
						{
							"name": "condition Satisfied",
							"type": "Wait",
							"dependsOn": [],
							"userProperties": [],
							"typeProperties": {
								"waitTimeInSeconds": 30
							}
						}
					]
				}
			},
			{
				"name": "daily war sink lookup",
				"type": "Lookup",
				"dependsOn": [
					{
						"activity": "test daily wars",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "ParquetSource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "ParquetReadSettings"
						}
					},
					"dataset": {
						"referenceName": "war_not_ended_data",
						"type": "DatasetReference"
					},
					"firstRowOnly": true
				}
			}
		],
		"annotations": []
	}
}